CREATE TABLE EMPLOYEE(EMP_NBR DECIMAL(5) PRIMARY KEY,EMP_EMP_NBR DECIMAL(5),EMP_FNAME VARCHAR(30),EMP_LNAME VARCHAR(30),CODE VARCHAR(6),EMP_SAL DECIMAL(8),EMP_TEL_NBR VARCHAR(15),STREET VARCHAR(10),CITY VARCHAR(15),STATE VARCHAR(5),ZIP VARCHAR(10),COUNTRY VARCHAR(10))
CREATE TABLE CD_DESC_TBL(CODE VARCHAR(6) PRIMARY KEY,CODE_DESC VARCHAR(30))
CREATE TABLE EMP_CD_TBL(EMP_NBR DECIMAL(5),PROJ_NBR DECIMAL(5),CODE VARCHAR(6),CONSTRAINT SYSTEM_PK PRIMARY KEY(EMP_NBR,PROJ_NBR,CODE))
CREATE TABLE PARTICIPATE(EMP_NBR DECIMAL(5),PROJ_NBR DECIMAL(5),TASK_NBR SMALLINT,BEG_DT DATE,END_DT DATE,CONSTRAINT SYSTEM_PK PRIMARY KEY(EMP_NBR,PROJ_NBR,TASK_NBR))
CREATE TABLE TASK(PROJ_NBR DECIMAL(5),TASK_NBR SMALLINT,BEG_DT DATE,END_DT DATE,TSK_COST DECIMAL(8),CONSTRAINT SYSTEM_PK PRIMARY KEY(PROJ_NBR,TASK_NBR))
CREATE TABLE TASK_MASTER(TASK_NBR SMALLINT PRIMARY KEY,TASK_NAME VARCHAR(20),TASK_DESC VARCHAR(30))
CREATE TABLE PROJECT(PROJ_NBR DECIMAL(5) PRIMARY KEY,PROJ_NM VARCHAR(30),BEG_DT DATE,END_DT DATE,PROJ_LBL VARCHAR(80))
